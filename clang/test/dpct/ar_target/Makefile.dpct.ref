// CHECK:TARGET_0_OBJ_1 = ./two.o
// CHECK:TARGET_0_FLAG_1 = ${FLAGS}
// CHECK:TARGET_1_SRC_0 = ./one.dp.cpp
// CHECK:TARGET_1_OBJ_0 = ./one.dp.o
// CHECK:TARGET_1_FLAG_0 = ${FLAGS}
// CHECK:TARGET_1_SRC_1 = ./two.cpp
// CHECK:TARGET_1_OBJ_1 = ./two.o
// CHECK:TARGET_1_FLAG_1 = ${FLAGS}
// CHECK:TARGET_1_SRC_2 = ./three.cpp
// CHECK:TARGET_1_OBJ_2 = ./three.o
// CHECK:TARGET_1_FLAG_2 = ${FLAGS}
// CHECK:TARGET_0 := ./foo
// CHECK:TARGET_1 := ./full.a
// CHECK:TARGET :=  ${TARGET_0} ${TARGET_1}
// CHECK:.PHONY:all clean
// CHECK:OBJS_0 :=  ${TARGET_0_OBJ_0} ${TARGET_0_OBJ_1}
