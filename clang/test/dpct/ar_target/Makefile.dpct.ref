// CHECK:INCLUDE_SYCL := $(ROOT_DIR)/../include
// CHECK:INCLUDE_CL   := $(ROOT_DIR)/../include/sycl
// CHECK:TARGET_0_SRC_0 = ./one.dp.cpp
// CHECK:TARGET_0_OBJ_0 = ./one.dp.o
// CHECK:TARGET_0_FLAG_0 = ${FLAGS}
